{
  "manifest_version": 3,
  "name": "Hello Extensions",
  "description": "Base Level Extension",
  "version": "1.0",
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_icon": "assets/hello_extensions.png"
  },
  "host_permissions": [
    "*://*/*"
  ],
  "permissions": [
    "tabs",
    "webNavigation",
    "scripting"
  ]
}